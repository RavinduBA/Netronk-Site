{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~components/*": ["./src/components/*"],
      "~styles/*": ["./src/styles/*"],
      "~assets/*": ["./src/assets/*"],
      "~layouts/*": ["./src/layouts/*"],
      "~lib/*": ["./src/lib/*"],
      "~/*": ["./src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
